else
  export NODE_PATH="/home/troll/ouorz-mono/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.5_ethers@6.12.1_hard_63akbs6tq32h3hre3lebknzl5u/node_modules/@openzeppelin/hardhat-upgrades/dist/scripts/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.5_ethers@6.12.1_hard_63akbs6tq32h3hre3lebknzl5u/node_modules/@openzeppelin/hardhat-upgrades/dist/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.5_ethers@6.12.1_hard_63akbs6tq32h3hre3lebknzl5u/node_modules/@openzeppelin/hardhat-upgrades/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.5_ethers@6.12.1_hard_63akbs6tq32h3hre3lebknzl5u/node_modules/@openzeppelin/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@2.5.1_@nomicfoundation+hardhat-ethers@3.0.5_ethers@6.12.1_hard_63akbs6tq32h3hre3lebknzl5u/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/node_modules:$NODE_PATH"
