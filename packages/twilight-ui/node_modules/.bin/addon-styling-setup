#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/troll/ouorz-mono/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react-dom@18.3.1_@types+react@18.3.2_less@4.2.0_postcss_ahozzppcotvyhevqygmrglnryi/node_modules/@storybook/addon-styling/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react-dom@18.3.1_@types+react@18.3.2_less@4.2.0_postcss_ahozzppcotvyhevqygmrglnryi/node_modules/@storybook/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react-dom@18.3.1_@types+react@18.3.2_less@4.2.0_postcss_ahozzppcotvyhevqygmrglnryi/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/troll/ouorz-mono/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react-dom@18.3.1_@types+react@18.3.2_less@4.2.0_postcss_ahozzppcotvyhevqygmrglnryi/node_modules/@storybook/addon-styling/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react-dom@18.3.1_@types+react@18.3.2_less@4.2.0_postcss_ahozzppcotvyhevqygmrglnryi/node_modules/@storybook/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/@storybook+addon-styling@1.3.7_@types+react-dom@18.3.1_@types+react@18.3.2_less@4.2.0_postcss_ahozzppcotvyhevqygmrglnryi/node_modules:/home/troll/ouorz-mono/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@storybook/addon-styling/postinstall.js" "$@"
else
  exec node  "$basedir/../@storybook/addon-styling/postinstall.js" "$@"
fi
